{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "env": { "browser": true },
  "rules": {
    "no-nested-ternary": "warn",
    "no-plusplus": "off",
    "react/destructuring-assignment": "off",
    "react/jsx-filename-extension": "off",
    "react/no-unused-state": "warn",
    "react/prop-types": "off",
  }
}
